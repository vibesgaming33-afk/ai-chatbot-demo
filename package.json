{
  "name": "ai-chatbot-demo",
  "private": true,
  "workspaces": ["apps/*"],
  "scripts": {
    "dev:web": "npm run dev -w apps/web",
    "dev:server": "npm run dev -w apps/server",
    "dev": "npm-run-all --parallel dev:server dev:web",
    "build": "npm run build -w apps/server && npm run build -w apps/web",
    "start": "npm start -w apps/server"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
